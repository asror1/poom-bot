{
  "name": "poom-bot",
  "version": "beta",
  "description": "Poom, lets you set up pomodoro timers with customizable intervals and rewards.",
  "scripts": {
    "test": "jest --watch --coverage --verbose",
    "start": "ts-node -r tsconfig-paths/register src/Bot.ts dev",
    "deploy": "ts-node -r tsconfig-paths/register src/Bot.ts prod",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/Bot.ts dev",
    "format": "prettier --write \"**/*.{js,ts,json}\"",
    "lint": "eslint --fix \"**/*.{js,ts}\""
  },
  "author": "asror",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.8.0",
    "dotenv": "^16.3.1",
    "nodemon": "^2.0.19",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.47.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
